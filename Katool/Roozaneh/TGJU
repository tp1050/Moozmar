		function open_support_bot() {
			if ( $('#gs-sdk').length ) {
				tc.funcs.toggleWidget(true);
				$('#fab-btn').click();
			} else {
				var this_script = document.createElement('script');
				this_script.type = 'text/javascript';
				this_script.id = 'gs-sdk';
				this_script.setAttribute('key', '3d2303a6-e680-4e91-b194-4b8387c7d0fe');
				this_script.src = '//www.buildquickbots.com/botwidget/v3/demo/static/js/sdk.js?v=3';
				document.body.appendChild(this_script);

				var check_support_robot_shown = setInterval(function () {
					if ( $('#supportchatwidget').length ) {
						tc.funcs.toggleWidget(true);
						clearInterval(check_support_robot_shown);
					}
				}, 100);

				$('#fab-btn').click();
			}
		}

		$(document).ready(function () {
			$(document).on('click', '#fab-btn', function () {
				$('.zoom-btn-sm').toggleClass('scale-out');
				$('#fab-btn').toggleClass('open');
			});
		});
	</script>
