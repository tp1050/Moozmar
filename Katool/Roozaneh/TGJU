		function ga_event(element, category, shop) {
			if ("ga" in window) {
				var tracker = ga.getAll()[0];
				if (tracker) {
					tracker.send({
						hitType: 'event',
						eventCategory: category,
						eventAction: shop,
						eventLabel: $(element).attr('href'),
						hitCallback : function () {
							console.log("Event received", category, shop);
						}
					});
				}
			}
		}


